{
  "name": "webapp-starter-kit",
  "version": "0.6.0",
  "description": "Starter kit for web front-end projects (React, react-apollo, Typescript).",
  "keywords": [
    "typescript",
    "react",
    "react-apollo",
    "graphQL",
    "ant design"
  ],
  "homepage": "https://github.com/fnstack/WebApp-starter-kit",
  "author": "FnStack team",
  "license": "MIT",
  "scripts": {
    "prestart": "node tools/startMessage.js",
    "start": "cross-env NODE_ENV=development && webpack-dev-server --config webpack.dev.js --mode development",
    "prelint": "esw --ext .js, webpack.*.* tools --color",
    "lint": "tslint -t stylish src/**/*.ts{,x}",
    "lint:fix": "tslint -t stylish src/**/*.ts{,x} --fix",
    "clean:dist": "rimraf ./dist",
    "clean:coverage": "rimraf ./coverage",
    "clean:lib": "rimraf ./node_modules",
    "prebuild": "npm-run-all clean:dist",
    "build": "webpack --config webpack.prod.js --mode production",
    "build:run": "npm run build && node tools/distServer.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:update": "cross-env NODE_ENV=test jest --updateSnapshot",
    "test:watch": "npm run test -- --watch",
    "generate": "gql-gen --config codegen.yml",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "npm run build && rimraf storybook-static && build-storybook",
    "doc": "docz dev",
    "doc:build": "docz build",
    "prettier:write": "prettier --write \"{src,__{tests,mocks}__}/**/*.ts{,x}\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@jbuschke/formik-antd": "^1.2.0",
    "@types/react-infinite-scroller": "^1.2.1",
    "antd": "^3.20.0",
    "apollo-boost": "^0.4.3",
    "apollo-client": "^2.6.3",
    "apollo-link-context": "^1.0.18",
    "apollo-link-state": "^0.4.2",
    "apollo-link-ws": "^1.0.18",
    "classnames": "^2.2.6",
    "formik": "^1.5.7",
    "graphql": "^14.4.1",
    "history": "^4.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-infinite-scroller": "^1.2.4",
    "react-intl": "^2.9.0",
    "react-loadable": "^5.5.0",
    "react-oidc": "^0.3.2",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2",
    "subscriptions-transport-ws": "^0.9.16",
    "ts-loader": "^6.0.4",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/chai": "^4.1.7",
    "@types/classnames": "^2.2.8",
    "@types/faker": "^4.1.5",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-cropper": "^0.10.6",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "5.0.8",
    "@types/react-intl": "2.3.18",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router": "5.0.2",
    "@types/react-router-dom": "4.3.4",
    "@types/webpack": "^4.4.34",
    "@types/webpack-env": "^1.13.9",
    "add": "^2.0.6",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.0.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^6.0.1",
    "eslint-watch": "^5.1.2",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-typescript-client": "^0.18.2",
    "graphql-codegen-typescript-common": "^0.18.2",
    "graphql-codegen-typescript-react-apollo": "^0.18.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^5.0.0",
    "immutable": "^4.0.0-rc.12",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "open": "^6.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "raf": "^3.4.1",
    "react-hot-loader": "^4.12.0",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "seamless-immutable": "^7.1.4",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "thread-loader": "^2.1.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "^3.6.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.1.1",
    "typescript": "^3.5.2",
    "typescript-eslint-parser": "^22.0.0",
    "url-loader": "^2.0.1",
    "waait": "^1.0.5",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "webpack-simple-progress-plugin": "^0.0.4"
  },
  "resolutions": {
    "@types/react": "^16.8.23"
  },
  "lint-staged": {
    "*.ts{,x}": [
      "prettier --write \"{src,__{tests,mocks}__}/**/*.ts{,x}\"",
      "git add",
      "tslint -t stylish \"src/**/*.ts{,x}\""
    ],
    "*.js": [
      "prettier --write",
      "git add",
      "esw --ext .js, webpack.*.* tools --color"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
