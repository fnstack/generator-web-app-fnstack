{
  "name": "webapp-starter-kit",
  "version": "0.5.0",
  "description": "Starter kit for web front-end projects (React, react-apollo, Typescript).",
  "keywords": [
    "typescript",
    "react",
    "react-apollo",
    "graphQL",
    "ant design"
  ],
  "homepage": "https://bitbucket.org/fnstack-frontend-team/dgddi-vms-web/src",
  "author": "Fn Stacks Dev team",
  "license": "MIT",
  "scripts": {
    "prestart": "node tools/startMessage.js",
    "start": "cross-env NODE_ENV=development && webpack-dev-server --config webpack.dev.js --mode development",
    "prelint": "esw --ext .js, webpack.*.* tools --color",
    "lint": "tslint -t stylish src/**/*.ts{,x}",
    "clean-dist": "rimraf ./dist",
    "clean-coverage": "rimraf ./coverage",
    "clean-lib": "rimraf ./node_modules",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "webpack --config webpack.prod.js --mode production",
    "postbuild": "node tools/distServer.js",
    "test": "cross-env NODE_ENV=test jest",
    "test-coverage": "cross-env NODE_ENV=test jest --coverage",
    "test-update": "cross-env NODE_ENV=test jest --updateSnapshot",
    "test-watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "antd": "^3.11.2",
    "apollo-boost": "^0.1.22",
    "apollo-client": "^2.4.7",
    "apollo-link-ws": "^1.0.10",
    "apollo-upload-client": "^9.1.0",
    "classnames": "^2.2.6",
    "graphql": "^14.0.2",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-apollo": "^2.3.2",
    "react-autocomplete": "^1.8.1",
    "react-cropper": "^1.0.1",
    "react-css-modules": "^4.7.8",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-infinite-scroller": "^1.2.2",
    "react-intl": "^2.7.2",
    "react-loadable": "^5.5.0",
    "react-oidc": "^0.1.0",
    "react-router-dom": "^4.3.1",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/chai": "^4.1.7",
    "@types/classnames": "^2.2.6",
    "@types/faker": "^4.1.4",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-addons-test-utils": "^0.14.23",
    "@types/react-cropper": "^0.10.3",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "5.0.7",
    "@types/react-intl": "2.3.14",
    "@types/react-loadable": "^5.4.2",
    "@types/react-router": "4.4.1",
    "@types/react-router-dom": "4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/react-transition-group": "^2.0.14",
    "@types/webpack": "^4.4.21",
    "@types/webpack-env": "^1.13.6",
    "add": "^2.0.6",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "cache-loader": "^1.2.5",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.10.0",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.6.0",
    "immutable": "^4.0.0-rc.12",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "open": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.3",
    "raf": "^3.4.1",
    "react-hot-loader": "^4.3.12",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "seamless-immutable": "^7.1.4",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "thread-loader": "^1.2.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-loader": "^3.6.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2",
    "url-loader": "^1.1.2",
    "waait": "^1.0.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.5",
    "webpack-simple-progress-plugin": "^0.0.4"
  },
  "lint-staged": {
    "*.ts{,x}": [
      "prettier --write \"{src,__{tests,mocks}__}/**/*.ts{,x}\"",
      "git add",
      "tslint -t stylish \"src/**/*.ts{,x}\""
    ],
    "*.js": [
      "prettier --write",
      "git add",
      "esw --ext .js, webpack.*.* tools --color"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
